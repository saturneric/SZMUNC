<style type="text/css">
    .atest{ width:85%;margin:0 auto} 
</style>
<div class="wrap ">
<div class="subMenu" >
    	<div class="container">
        	<div class="row">
            	<div class="col-md-2 th-logo">
                	<a href="#sTop">
                    	<img src="/img/logo.png" alt="logo" />
                    </a>
                </div>
                <div class="col-md-10">
                    <div class="inner pull-right">
                        <% Object.keys(theme.menu).forEach(function(key) { %>
                        <a href="<%=theme.menu[key]%>" class="subNavBtn"><%=key%></a>
                         <% }); %>
                    </div>
            	</div>
            </div><!-- row -->
        </div>
	</div>
	<div class="section s1">
		<div class="inner ">
			<div class="about-artical">
  				<article id="post">
 				 <h1><%=page.title%></h1>
 				 <br />
            </div><!-- /about-artical -->
            <div class="atest">
                <%-page.content%>
                </article>
                <%-partial('partials/pagination')%>
            </div>
		</div>
</div>


